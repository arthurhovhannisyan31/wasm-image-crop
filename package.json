{
  "name": "hello-wasm",
  "packageManager": "yarn@4.7.0",
  "engines": {
    "node": "^22.9.0"
  },
  "scripts": {
    "dev": "webpack serve --config configs/webpack/webpack.dev.js",
    "watch:rs": "nodemon --polling-interval 500 -e rs --exec \"wasm-pack build --out-dir wasm-pkg\"",
    "serve": "nodemon -r dotenv/config --config configs/nodemon-prod.json server.js",
    "build": "./configs/scripts/build.sh",
    "build:fe": "__PROD__=true webpack --config configs/webpack/webpack.prod.js",
    "prepare": "corepack install",
    "lint": "yarn eslint '**/*.{js,ts,tsx}' --cache --quiet",
    "lint:fix": "yarn lint:warn --fix",
    "types": "NODE_OPTIONS=--max-old-space-size=8196 tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.6",
    "@mui/lab": "^6.0.0-beta.29",
    "@mui/material": "^6.4.6",
    "@mui/styled-engine": "^6.4.6",
    "@mui/system": "^6.4.6",
    "@mui/types": "^7.2.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "wasm-pkg": "file:./wasm-pkg"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin": "^4.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.27.0",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.2.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-deprecate": "^0.8.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "file-loader": "^6.2.0",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^9",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.12",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0",
    "webpack-merge": "^5.8.0"
  }
}
