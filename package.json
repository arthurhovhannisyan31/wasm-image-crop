{
  "name": "wasm-image-crop",
  "packageManager": "yarn@4.7.0",
  "engines": {
    "node": "^22.9.0"
  },
  "scripts": {
    "build": "yarn build:rs && yarn build:fe",
    "build:rs": "configs/scripts/build-wasm.sh",
    "build:fe": "configs/scripts/build-web.sh",
    "dev": "webpack serve --config configs/webpack/webpack.dev.js",
    "watch:rs": "nodemon --polling-interval 1000 -e rs --exec \"yarn build:rs\"",
    "serve": "nodemon -r dotenv/config --config configs/nodemon-prod.json server.js",
    "prepare": "corepack install && husky",
    "lint": "yarn eslint '**/*.{js,ts,tsx}' --cache --quiet",
    "lint:warn": "yarn lint --max-warnings 100",
    "lint:staged": "./configs/scripts/lint-staged.sh",
    "types": "NODE_OPTIONS=--max-old-space-size=8196 tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@arthurhovhannisyan31/simple-service-worker": "^1.0.2",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.6",
    "@mui/lab": "^6.0.0-beta.29",
    "@mui/material": "^6.4.6",
    "@mui/styled-engine": "^6.4.6",
    "@mui/system": "^6.4.6",
    "@mui/types": "^7.2.21",
    "lodash-es": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin": "^4.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.27.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.2.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-deprecate": "^0.8.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "file-loader": "^6.2.0",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.12",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "webpack": "^5.99.6",
    "webpack-assets-manifest": "^6.0.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1"
  }
}
